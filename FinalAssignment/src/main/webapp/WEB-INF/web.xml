<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ImageUpload</servlet-name>
  	<display-name>ImageUpload</display-name>
  	<description></description>
  	<servlet-class>com.accolite.controllers.ImageUpload</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>newCourse</servlet-name>
  	<display-name>newCourse</display-name>
  	<description></description>
  	<servlet-class>com.accolite.controllers.newCourse</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>updateCourse</servlet-name>
  	<display-name>updateCourse</display-name>
  	<description></description>
  	<servlet-class>com.accolite.controllers.updateCourse</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>updateInstitute</servlet-name>
  	<display-name>updateInstitute</display-name>
  	<description></description>
  	<servlet-class>com.accolite.controllers.updateInstitute</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ImageUpload</servlet-name>
  	<url-pattern>/ImageUpload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>newCourse</servlet-name>
  	<url-pattern>/newCourse</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>updateCourse</servlet-name>
  	<url-pattern>/updateCourse</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>updateInstitute</servlet-name>
  	<url-pattern>/updateInstitute</url-pattern>
  </servlet-mapping>
</web-app>
